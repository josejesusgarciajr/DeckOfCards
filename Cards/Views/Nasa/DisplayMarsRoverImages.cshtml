
@{
    ViewData["Title"] = "Mars Photos";
}

<div class="text-center">
    <h1 class="lead">NASA PHOTOS</h1>
</div>

<div class="container">
    @foreach(NasaPhoto nasaPhoto in Model)
    {
        <a asp-controller="Nasa" asp-action="NasaPhotoInfo"
           asp-route-photoID="@nasaPhoto.id">
            <img src="@nasaPhoto.img_src"
                 width="150" height="150"/>
        </a>
    }
</div>
